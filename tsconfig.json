{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es2017", "es2018", "dom", "esnext.asynciterable"],
    "module": "commonjs",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "noImplicitAny": false,
    "noExplicitAny": false,
    "paths": {
      "Constants/*": ["src/constants/*"],
      "Components/*": ["src/components/*"],
      "Static/*": ["src/static/*"],
      "Styles/*": ["src/styles/*"],
      "Templates/*": ["src/templates/*"],
      "Hooks/*": ["src/hooks/*"],
      "Hocs/*": ["src/hocs/*"]
    }
  }
}
